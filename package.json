{
  "name": "@agenticmail/enterprise",
  "version": "0.3.0",
  "description": "AgenticMail Enterprise â€” cloud-hosted AI agent identity, email, auth & compliance for organizations",
  "type": "module",
  "bin": {
    "agenticmail-enterprise": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/cli.ts --format esm --external better-sqlite3 --external mongodb --external mysql2 --external @libsql/client --external @aws-sdk/client-dynamodb --external @aws-sdk/lib-dynamodb && mkdir -p dist/dashboard && cp src/dashboard/index.html dist/dashboard/",
    "dev": "tsx src/cli.ts"
  },
  "keywords": [
    "ai",
    "agent",
    "email",
    "enterprise",
    "saml",
    "oidc",
    "scim",
    "identity"
  ],
  "license": "MIT",
  "peerDependencies": {
    "openclaw": ">=0.1.0",
    "agenticmail": ">=0.5.0",
    "@agenticmail/core": ">=0.5.0",
    "@agenticmail/api": ">=0.5.0"
  },
  "peerDependenciesMeta": {
    "openclaw": {
      "optional": true
    },
    "agenticmail": {
      "optional": true
    },
    "@agenticmail/api": {
      "optional": true
    }
  },
  "dependencies": {
    "@hono/node-server": "^1.19.9",
    "bcryptjs": "^2.4.3",
    "chalk": "^5.0.0",
    "hono": "^4.0.0",
    "inquirer": "^9.0.0",
    "jose": "^5.0.0",
    "nanoid": "^5.0.0",
    "ora": "^8.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.0",
    "@types/inquirer": "^9.0.0",
    "@types/node": "^22.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
